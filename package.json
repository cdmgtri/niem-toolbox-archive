{
  "name": "niem-toolbox",
  "version": "1.0.0",
  "description": "Capabilities to support NIEM modeling and development work.",
  "scripts": {
    "assets": "vue-asset-generate -a src/assets/images/logo.2.png -o public/img/icons",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "pm2": "pm2 start ecosystem.config.yaml",
    "pm2:list": "pm2 list",
    "pm2:monit": "pm2 monit",
    "pm2:restart": "pm2 restart niem-toolbox",
    "pm2:stop": "pm2 stop niem-toolbox",
    "pm2:delete": "pm2 delete niem-toolbox",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "deploy": "npm run build && npm run dist:commit && npm run dist:push",
    "dist:commit": "git add --force dist && git commit -m \"Updated dist\" || true",
    "dist:push": "bash src/scripts/git/push-dist.sh",
    "reload": "bash src/scripts/copy-release.sh"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.16.0",
    "core-js": "^3.6.5",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "niem-model": "github:cdmgtri/niem-model#dev",
    "pm2": "^4.4.0",
    "popper.js": "^1.16.1",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.3.1",
    "vue-router": "^3.4.3",
    "vue-worker": "^1.2.1",
    "vuewordcloud": "^18.7.12",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.3",
    "@vue/cli-plugin-eslint": "^4.5.3",
    "@vue/cli-plugin-pwa": "^4.5.3",
    "@vue/cli-plugin-router": "^4.5.3",
    "@vue/cli-plugin-unit-jest": "^4.5.3",
    "@vue/cli-plugin-vuex": "^4.5.3",
    "@vue/cli-service": "^4.5.3",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "comlink-loader": "^2.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.14.1",
    "sass-loader": "^9.0.3",
    "vue-pwa-asset-generator": "^1.3.0",
    "vue-template-compiler": "^2.6.11"
  }
}
